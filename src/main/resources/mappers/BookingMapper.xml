<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.hi1856.lesson06.dao.BookingDAO">
 	<select id="bookingList" resultType="com.hi1856.lesson06.model.Booking">
 		SELECT
		`name`
		,`headcount`
		,`day`
		,`date`
		,`phoneNumber`
		,`state`
		,`createdAt`
		,`updatedAt`
 		FROM
 			`booking`
 	</select>
 
 </mapper>