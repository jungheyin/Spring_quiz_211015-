<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.hi1856.lesson04.dao.SellerDAO">
 	<insert id="insertSeller" parameterType="map">
 		INSERT INTO `seller`
 		(
		 	`id`
			,`nickname`
			,`profileImageUrl`
			,`temperature`
			,`createdAt`
			,`updatedAt`
 		)
 		VALUES
 		(
 			#{id}
 			,#{nickname}
 			,#{profileImageUrl}
 			,#{temperature}
 			,NOW()
 			,NOW()
 		)
 		
 	</insert>
 
 </mapper>